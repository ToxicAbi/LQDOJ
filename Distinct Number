#include<bits/stdc++.h>
#define ll long long
#define input(a) cin >> a; cin.ignore();
#define output(a) cout << a
#define FOR(i, a, b) for(ll i = a; i <= b; i++)
#define FOD(i, a, b) for(ll i = a; i >= b; i--)
#define vt(a) vector<a>
#define st(a) stack<a>
#define qu(a) queue<a>
#define st(a) set<a>
#define IOS ios_base::sync_with_stdio(false); cin.tie(0)
using namespace std;

ll n;
ll a[10000009];
st(ll) s;

int main() {
    IOS;
    input(n);
    
    FOR(i, 1, n){
        input(a[i]);
        s.insert(a[i]);
    }

    output(s.size());
    
    return 0;
}
