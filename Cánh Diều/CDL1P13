#include<bits/stdc++.h>
#define ll long long
#define input(a) cin >> a; cin.ignore()
#define output(a) cout << a
#define FOR(i, a, b) for(ll i = a; i <= b; i++)
#define FOD(i, a, b) for(ll i = a; i >= b; i--)
#define vt(a) vector<a>
#define stk(a) stack<a>
#define qu(a) queue<a>
#define st(a) set<a>
#define pa(a, b) pair<a, b>
#define IOS ios_base::sync_with_stdio(false); cin.tie(0)
#define pb push_back
#define mp make_pair
using namespace std;

ll n;
ll k;

int main() {
    IOS;
    input(n);
    input(k);
    printf("Moi hoc sinh duoc chia %lld qua man.\nSo man danh rieng cho cac em nu la %lld.", k / n, k % n);
    return 0;
}

