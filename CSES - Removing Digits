#include<bits/stdc++.h>
#define ll long long
#define input(a) cin >> a;
#define output(a) cout << a
#define FOR(i, a, b) for(ll i = a; i <= b; i++)
#define FOD(i, a, b) for(ll i = a; i >= b; i--)
#define vt(a) vector<a>
#define st(a) stack<a>
#define qu(a) queue<a>
#define IOS ios_base::sync_with_stdio(false); cin.tie(0)
using namespace std;

ll n;
ll res = 0;

ll solve(ll n){
    ll m = 0;
    while(n != 0){
        m = max(m, n % 10);
        n /= 10;
    }

    return m;
}

int main() {
    IOS;
    input(n);

    while(n != 0){
        res++;
        n = n - solve(n);
    }

    output(res);
    return 0;
}
